grammar org.xtext.wiz.Clautext with org.eclipse.xtext.common.Terminals

generate clautext "http://www.xtext.org/wiz/Clautext"


Lang :
	lang += Clautext*
;

Clautext :
	Event |
	Data  |
	Main 
;

Data : // WIZ_STRING -> STRINGS??
	val=STRINGS |
	var=STRINGS '=' val=STRINGS |
	var=STRINGS '=' '{' data = Data2* '}'
;

Data2 : // WIZ_STRING -> STRINGS??
	val=STRINGS_ALL |
	var=STRINGS_ALL '=' val=STRINGS_ALL |
	var=STRINGS_ALL '=' '{' data = Data2* '}'
;

Main :
	'Main' '=' '{'
		'$call' '=' '{' 'id' '=' id=WIZ_STRING '}' 
	'}'	
;

Event  :
	'Event' '=' '{'
		event = EventData+
	'}'
;
EventData :
	'id' '=' val=WIZ_STRING |
	Func |
	'$local' '=' '{'
		WIZ_STRING+
	'}' |
	'$parameter' '=' '{'
		WIZ_STRING+ 
	'}'
;
Func:
	'$call' '=' '{' 
		CallData+
	'}' |
	'$print' '=' '{' // $print -> $print, $print_list?
		 WIZ_STRING '=' '{'
			Expr // check STRINGS?
		'}'
		( WIZ_STRING '=' '{'
			Expr
		'}'
		 WIZ_STRING '=' '{'
			Expr
		'}' )?
	'}' |
	'$while' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Func*
		'}'
	'}' |
	'$do' '=' '{'
		WIZ_STRING '=' '{'
			STRINGS
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$if' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Func*
		'}'
	'}'
	(	'$else' '=' '{' 
			WIZ_STRING '=' '{'
				Func*
			'}'
		'}'
	)? |
	'$_getch' '=' '{' '}' |
	'$replace_datatype1' '=' '{'
		(WIZ_STRING '=' '{'
			Expr
		'}')+
	'}' |
	'$replace_datatype1_2' '=' '{'
		(WIZ_STRING '=' '{'
			Expr
		'}')+
	'}' |
	'$replace_datatype2' '=' '{'
		(WIZ_STRING '=' '{' 
			Expr
		'}')+
	'}' |
	'$replace_datetype' '=' '{'
		WIZ_STRING '=' '{'
			WIZ_STRING
		'}'
		(WIZ_STRING '=' '{'
			Expr
		'}')+
	'}' |
	'$replace_datetype2' '=' '{'
		WIZ_STRING '=' '{'
			WIZ_STRING
		'}'
		(WIZ_STRING '=' '{'
			Expr
		'}')+
	'}' |
	'$remove_usertype_total' '=' '{'
		(WIZ_STRING '=' '{'
			Expr
		'}')+
	'}' |
	'$replace_item' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			WIZ_STRING
		'}'
		(WIZ_STRING '=' '{'
			Expr
		'}')+
	'}' |
	'$edit_mode' '=' '{'
	'}' |
	'$save' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$save_data_only' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$save_data_only2' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$register_module' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$call_registered_module' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		(WIZ_STRING '=' '{'
			Expr
		'}')?
	'}' |
	'$module' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		(WIZ_STRING '=' '{'
			Expr
		'}')?
	'}' |
	'$register_object' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$copy_object' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$register_object_from_string' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$call_registered_object' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$call_registered_object2' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$object' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$object_from_string' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}' |
	'$option' '=' '{'
		WIZ_STRING '=' '{'
			Expr
		'}'
	'}'
;

CallData :
	'id' '=' WIZ_STRING |
	'id' '=' '{' 
		Expr
	'}' |
	WIZ_STRING '=' WIZ_STRING |
	WIZ_STRING '=' '{' 
		Expr
	'}'
;

Expr:
	'$EQ' '=' '{'
		Expr Expr
	'}' |
	'$COMP>' '=' '{'
		Expr Expr
	'}' |
	'$COMP<' '=' '{'
		Expr Expr
	'}' |
	'$COMP>EQ' '=' '{'
		Expr Expr
	'}' |
	'$COMP<EQ' '=' '{'
		Expr Expr
	'}' |
	'$concat_all' '=' '{'
		Expr*
	'}' |
	STRINGS
;

STRINGS_ALL :
	STRINGS |
	'Main' |
	'Event'
;
STRINGS:
	DIR_STRING |
	LOCAL_STRING|
	PARAMTER_STRING |
	WIZ_STRING |
	'TRUE' |
	'FALSE' |
	'$EQ' |
	'$COMP<' |
	'$COMP>' |
	'$COMP<EQ' |
	'$COMP>EQ' |
	'$concat_all' |
	'id' |
	'$call' |
	'$parameter' |
	'$print' |
	'$while' |
	'$do' |
	'$if' |
	'$else' |
	'$_getch' |
	'$replace_datatype1' |
	'$replace_datatype1_2' |
	'$replace_datatype2' |
	'$replace_datetype' |
	'$replace_datetype2' |
	'$remove_usertype_total' |
	'$replace_item' |
	'$edit_mode' |
	'$save' |
	'$save_data_only' |
	'$save_data_only2' |
	'$register_module' |
	'$call_registered_module' |
	'$module' |
	'$register_object' |
	'$copy_object' |
	'$register_object_from_string' |
	'$call_registered_object' |
	'$call_registered_object2' |
	'$object' |
	'$object_from_string' |
	'$option'
;

terminal DIR_STRING:
	'/' ('"' ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|'"'|'\r'|'\n') )* '"' |
	"'" ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|"'"|'\r'|'\n') )* "'" |
	(!(' '|'\t'|'\r'|'\n'|'#'|'"'|"'"))+)
;

terminal LOCAL_STRING:
	'$local.' ('"' ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|'"'|'\r'|'\n') )* '"' |
	"'" ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|"'"|'\r'|'\n') )* "'" |
	(!(' '|'\t'|'\r'|'\n'|'#'|'"'|"'"))+)	
;

terminal PARAMTER_STRING:
	'$parameter.' ('"' ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|'"'|'\r'|'\n') )* '"' |
	"'" ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|"'"|'\r'|'\n') )* "'" |
	(!(' '|'\t'|'\r'|'\n'|'#'|'"'|"'"))+)
;

terminal WIZ_STRING :  
	'"' ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|'"'|'\r'|'\n') )* '"' |
	"'" ( '\\' . /* 'b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\' */ | !('\\'|"'"|'\r'|'\n') )* "'" |
	(!(' '|'\t'|'\r'|'\n'|'#'|'"'|"'"))+
;

@Override
terminal SL_COMMENT 	: '#' !('\n'|'\r')* ('\r'? '\n')?;

